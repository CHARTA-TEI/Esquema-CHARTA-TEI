tei_w =
  
  ## (palabra) representa una palabra gramatical(no necesariamente ortográfica) [17.1. Linguistic Segment Categories 17.4.2. Lightweight Linguistic Annotation]
  element w {
    (text
     | tei_model.gLike
     | tei_seg
     | tei_w
     | tei_m
     | tei_c
     | tei_pc
     | tei_model.global
     | tei_model.lPart
     | tei_model.hiLike
     | tei_model.pPart.edit)*,
    tei_att.global.attributes,
    tei_att.segLike.attributes,
    tei_att.typed.attributes,
    tei_att.linguistic.attributes,
    tei_att.notated.attributes,
    empty
  }
tei_m =
  
  ## (morfema) representa un morfema gramatical. [17.1. Linguistic Segment Categories]
  element m {
    (text
     | tei_model.gLike
     | tei_model.hiLike
     | tei_seg
     | tei_m
     | tei_c
     | tei_model.global)*,
    tei_att.global.attributes,
    tei_att.segLike.attributes,
    tei_att.typed.attributes,
    tei_att.notated.attributes,
    
    ## identifica la forma base del morfema
    attribute baseForm {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    empty
  }
tei_c =
  
  ## (carácter) representa un carácter gráfico. [17.1. Linguistic Segment Categories]
  element c {
    tei_macro.xtext,
    tei_att.global.attributes,
